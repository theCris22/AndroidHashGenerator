<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.crisnavarro.androidhashgenerator.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_hashFragment"
            app:destination="@id/hashFragment" />
    </fragment>
    <fragment
        android:id="@+id/hashFragment"
        android:name="com.crisnavarro.androidhashgenerator.HashFragment"
        android:label="fragment_hash"
        tools:layout="@layout/fragment_hash">
        <action
            android:id="@+id/action_hashFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>